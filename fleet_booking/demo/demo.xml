<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
    <!--BRANCH-->
    <record model="fleet_branch.branch" id="main_branch">
        <field name="name">Main branch</field>
    </record>
    <record model="fleet_branch.branch" id="filial_branch">
        <field name="name">Filial branch</field>
    </record>
    <!--PARTNERS-->
    <record id="fb_gm_partner" model="res.partner">
        <field name="name">General Manager</field>
        <field name="email">gm@example.com</field>
        <field name="supplier" eval="False"/>
        <field name="customer" eval="False"/>
        <field name="notify_email">none</field>
        <field name="city">Medina</field>
    </record>
    <record id="fb_bo_partner" model="res.partner">
        <field name="name">Branch Officer</field>
        <field name="email">bo@example.com</field>
        <field name="supplier" eval="False"/>
        <field name="customer" eval="False"/>
        <field name="notify_email">none</field>
        <field name="city">Medina</field>
    </record>
    <record id="fb_bo2_partner" model="res.partner">
        <field name="name">Branch Officer 2</field>
        <field name="email">bo2@example.com</field>
        <field name="supplier" eval="False"/>
        <field name="customer" eval="False"/>
        <field name="notify_email">none</field>
        <field name="city">Medina</field>
    </record>
    <record id="fb_vso_partner" model="res.partner">
        <field name="name">Vehicles Support Officer</field>
        <field name="email">vso@example.com</field>
        <field name="supplier" eval="False"/>
        <field name="customer" eval="False"/>
        <field name="notify_email">none</field>
        <field name="city">Medina</field>
    </record>
    <record id="fb_be_partner" model="res.partner">
        <field name="name">Branch Employee</field>
        <field name="email">be@example.com</field>
        <field name="supplier" eval="False"/>
        <field name="customer" eval="False"/>
        <field name="notify_email">none</field>
        <field name="city">Medina</field>
    </record>
    <record id="fb_ac_partner" model="res.partner">
        <field name="name">Accountant</field>
        <field name="email">ac@example.com</field>
        <field name="supplier" eval="False"/>
        <field name="customer" eval="False"/>
        <field name="notify_email">none</field>
        <field name="city">Medina</field>
    </record>
    <record id="fb_po_partner" model="res.partner">
        <field name="name">Payroll Officer</field>
        <field name="email">po@example.com</field>
        <field name="supplier" eval="False"/>
        <field name="customer" eval="False"/>
        <field name="notify_email">none</field>
        <field name="city">Medina</field>
    </record>
    <!--USERS-->
    <record id="fb_gm_user" model="res.users">
        <field name="partner_id" ref="fb_gm_partner"/>
        <field name="login">gm</field>
        <field name="password">gm</field>
        <field name="branch_id" ref="main_branch"/>
        <field name="groups_id" eval="[(4, ref('group_general_manager'))]"/>
    </record>
    <record id="fb_bo_user" model="res.users">
        <field name="partner_id" ref="fb_bo_partner"/>
        <field name="login">bo</field>
        <field name="password">bo</field>
        <field name="branch_id" ref="main_branch"/>
        <field name="groups_id" eval="[(4, ref('group_branch_officer'))]"/>
    </record>
    <record id="fb_bo2_user" model="res.users">
        <field name="partner_id" ref="fb_bo2_partner"/>
        <field name="login">bo2</field>
        <field name="password">bo2</field>
        <field name="branch_id" ref="filial_branch"/>
        <field name="groups_id" eval="[(4, ref('group_branch_officer'))]"/>
    </record>
    <record id="fb_vso_user" model="res.users">
        <field name="partner_id" ref="fb_vso_partner"/>
        <field name="login">vso</field>
        <field name="password">vso</field>
        <field name="branch_id" ref="main_branch"/>
        <field name="groups_id" eval="[(4, ref('group_vehicles_support_officer'))]"/>
    </record>
    <record id="fb_be_user" model="res.users">
        <field name="partner_id" ref="fb_be_partner"/>
        <field name="login">be</field>
        <field name="password">be</field>
        <field name="branch_id" ref="main_branch"/>
        <field name="groups_id" eval="[(4, ref('group_branch_employee'))]"/>
    </record>
    <record id="fb_ac_user" model="res.users">
        <field name="partner_id" ref="fb_ac_partner"/>
        <field name="login">ac</field>
        <field name="password">ac</field>
        <field name="branch_id" ref="main_branch"/>
        <field name="groups_id" eval="[(4, ref('group_accountant'))]"/>
    </record>
    <record id="fb_po_user" model="res.users">
        <field name="partner_id" ref="fb_po_partner"/>
        <field name="login">po</field>
        <field name="password">po</field>
        <field name="branch_id" ref="main_branch"/>
        <field name="groups_id" eval="[(4, ref('group_payroll_officer'))]"/>
    </record>
    <!--OTHER-->

</data>
</openerp>
